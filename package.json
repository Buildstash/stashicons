{
  "name": "stashicons",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "https://github.com/buildstash/stashicons.git",
  "files": [
    "20/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepublishOnly": "bun run build",
    "lint": "node ./scripts/verify-names.js",
    "prebuild": "rimraf ./{20} ./{react}/{20} ./optimized/{20}",
    "build": "bun run build-20 && bun run build-react",
    "build-react": "node ./scripts/build.js react",
    "build-20": "rimraf ./20 ./optimized/20 && svgo --config=svgo.20.mjs -f ./src/20 -o ./optimized/20 --pretty --indent=2 && mkdir -p ./20 && cp -R ./optimized/20 ./20",
    "release-channel": "node ./scripts/release-channel.js",
    "release-notes": "node ./scripts/release-notes.js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@svgr/core": "^5.5.0",
    "camelcase": "^6.0.0",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "svgo": "^3.0.2"
  }
}
